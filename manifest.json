{
    "name": "Quick Navigation Tiles",
    "description": "A google chrome extension for quick navigation tiles from the homepage!",
    "version": "1.0",
    "manifest_version": 3,
    "chrome_url_overrides": {
        "newtab": "QuickNavigationHomePage.html"
    },
    "content_scripts": [{
        "css": ["stylesheet.css"],
        "js": ["content.js"],
        "matches": ["https://www.google.co.uk/"]
    }],
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    },
    "action": {
        "default_icon": {
            "16": "/images/get_started16.png",
            "32": "/images/get_started32.png",
            "48": "/images/get_started48.png",
            "128": "/images/get_started128.png"
        }
    }
}